{
  "name": "@openlab/alembic",
  "version": "0.4.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/module.js",
  "module": "dist/module.js",
  "types": "dist/module.d.ts",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "require": "./dist/module.cjs",
      "default": "./dist/module.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.cjs",
      "default": "./dist/*"
    }
  },
  "scripts": {
    "build": "./scripts/build-library.js && ./scripts/build-docs.sh",
    "start": "NOVA_TASK_NAME=run npm run build",
    "test": "tsx --test **/*.test.*"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@types/svgo": "^2.6.3",
    "dedent": "^1.5.3",
    "esbuild": "^0.25.2",
    "markdown-it-anchor": "^8.6.6",
    "prism-themes": "^1.9.0",
    "slugify": "^1.6.6",
    "svgo": "^2.8.0",
    "ts-morph": "^17.0.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  }
}
